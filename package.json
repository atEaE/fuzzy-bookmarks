{
	"name": "fuzzy-bookmarks",
	"displayName": "fuzzy-bookmarks",
	"description": "fuzzy bookmarks",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "FzB",
				"title": "Search Bookmarks",
				"command": "fzb.searchBookmarks"
			}
		],
		"configuration": [
			{
				"title": "FuzzyBookmarks",
				"properties": {
					"fzb.defaultBookmarkDir": {
						"type": "string",
						"default": "~/.vscode/fzb",
						"description": "Set up a directory to save the bookmark."
					},
					"fzb.defaultBookmarkFileName": {
						"type": "string",
						"default": "bookmarks.json",
						"description": "Set a file name to save the bookmark."
					},
					"fzb.directoryOpenType": {
						"type": "string",
						"enum": [
							"terminal",
							"explorer"
						],
						"default": "terminal",
						"description": "Set the Open method when Bookmark is selected as the directory type."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"open": "^8.0.6"
	}
}